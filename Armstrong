public class Armstrong {
    public static void main(String[] args) {
        int num = 153, sum = 0, n = num;

        while (n > 0) {
            int d = n % 10;
            sum = sum + d*d*d;
            n = n / 10;
        }

        if (sum == num)
            System.out.println("Armstrong");
        else
            System.out.println("Not Armstrong");
    }
}
