public class Test{
public static void main(String[] args) {
int num = 1221;
int reverseNum = 0;
int temp = num;

while (temp != 0) {
int digit = temp % 10;
reverseNum = reverseNum * 10 + digit;
temp = temp / 10;
}
if (num == reverseNum) {
System.out.println(" Yes, "+ num +" is a palindrome number");
} 
else 
{
System.out.println(" No, "+ num +" is not a palindrome number.");
}
}
}
