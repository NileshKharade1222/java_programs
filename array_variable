package Array;

public class array_variable {
	public static void main(String[] args) {
		
		         char array_variable [] = new char[10];
	         for (int i = 0; i < 10; ++i) 
	         {
	              array_variable[i] = 'k';
	              System.out.print(array_variable[i] + "");
	          }
	
	
	}
}

